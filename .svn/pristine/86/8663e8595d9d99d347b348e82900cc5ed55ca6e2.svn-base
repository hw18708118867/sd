@charset "utf-8";
/* CSS Document */
html,body
{margin:0 auto;
padding:0;
border:0;
font-size:100%;width:100%;height:100%;
}
*{border:0;}
.div0
{width:100%;
height:100%;
}
.img0
{width:100%;
height:100%;
}
.div1
{width:40%;
height:40%;
position:absolute;
    top:0;
    left:0;
    bottom:0;right:0;margin:auto;
    overflow: hidden;
    +margin-left:30%;
    +margin-top:10%;
/*top:30.6%;
left:30.2%;*/
z-index:10;
    min-width:320px;
}
.img1
{
top:20.3%;
left:35.45%;
position:absolute;
z-index:11;
height:59%;
width:29.2%;
}
.div1a, .div1b, .div1c, .div1d
{width:100%;height:100%;display: block;
    background:url('../images/2.png') no-repeat center;
}

.div1b{background-image:url("../images/3.png")}
.div1c{background-image:url("../images/4.png")}
.div1d{background-image:url("../images/5.png")}

.P1, .P2, .P3, .P4, .P5, .P6
{font-size:16px;
line-height:1.5;
    margin:0;
font-family:"微软雅黑";
color:#ffffff;
    text-align:center;
    padding-top:80px;
}
.P2, .P4, .P6{padding-top:10px;}



