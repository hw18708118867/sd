@charset "utf-8";
/* CSS Document */
body{margin:0; padding:0;}
div{margin:0;}
.an_div1
{width:100%;
height:170px;
font-size:1.5em;
font-family:"微软雅黑";
line-height:500%;
text-align:center;
}
.an_div2
{width:85%;
min-height:430px;
margin:0 auto;
}
.an_div2a
{width:22%;
height:430px;
float:left;
margin:0 1.5%;
}
.an_div2a dl
{width:100%;
height:100%;
position:relative;
}
.an_div2adt1
{width:100%;
 height:67%;
border:1px solid transparent;
border-radius:10px;
-moz-border-radius:10px; /* Old Firefox */
}
.an_div2adt1 img
{
width:100%;
height:100%;
}
.an_div2adt1f
{width:100%;
 height:67%;
border:1px solid transparent;
border-radius:10px;
-moz-border-radius:10px; /* Old Firefox */
position:absolute;
top:0;
left:0;
z-index:10;
}
.an_div2adt1f1
{width:30.5%;
height:38%;
margin:23.5% auto 0 auto;
}
.an_div2adt1f1 img
{width:100%;
height:100%;}

.an_div2adt1ff
{
width:100%;
height:67%;
border:1px solid transparent;
background-color:#000;
border-radius:10px;
-moz-border-radius:10px; /* Old Firefox */
position:absolute;
top:0;
left:0;
z-index:9;
opacity:0.5;
}
.an_div2adt2
{width:100%;
 height:33%;
 overflow:hidden;
}
.an_div2adt2 p
{margin:0;
padding:1% 5%;
text-align:center;
font-size:1.25em;
font-family:"微软雅黑";
}
.bao
{display:none;}